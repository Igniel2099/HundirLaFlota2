<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane
        fx:id="fatherPane"
        maxHeight="-Infinity"
        maxWidth="-Infinity"
        minHeight="-Infinity"
        minWidth="-Infinity"
        prefHeight="750.0"
        prefWidth="1200.0"
        xmlns="http://javafx.com/javafx/23.0.1"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="org.example.hundirlaflota2.Controllers.VictoryController">
    <children>
        <ImageView
                fx:id="backgroundImageView"
                fitHeight="Infinity"
                fitWidth="Infinity"
                pickOnBounds="true">
            <image>
                <Image url="@../Images/BackgroundMain.png" />
            </image>
        </ImageView>
        <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <children>
                <ImageView fx:id="imageWinner" fitHeight="344.0" fitWidth="405.0" layoutX="462.0" layoutY="101.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="462.0" AnchorPane.topAnchor="101.0">
                    <image>
                        <Image url="@../Images/premio.png" />
                    </image>
                </ImageView>
                <Label fx:id="labelWinner"
                        layoutX="425.0" layoutY="398.0" text="Eres un ganador Walther" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="36.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
    </children>
</StackPane>
